# http://editorconfig.org
root = true

[*]
indent_style = tab                  #缩紧格式为tab
indent_size = 1                     #缩紧1格
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

[Makefile]
indent_style = tab

[*.scss]
indent_style = tab
