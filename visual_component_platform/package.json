{
  "name": "Visual_Component_Platform_Server",
  "description": "飞鱼大数据处理套件Node Server",
  "version": "1.7.0",
  "scripts": {
    "dev": "node dev.js",
    "dev-debug": "DEBUG=think-router node development.js",
    "pm2-production-start": "pm2 start pm2.production.json",
    "pm2-production-reload": "pm2 sendSignal SIGUSR2 Visual_Component_Platform_Server pm2.production.json",
    "pm2-production-restart": "pm2 restart pm2.production.json",
    "pm2-production-stop": "pm2 stop pm2.production.json",
    "pm2-staging-start": "pm2 start pm2.staging.json",
    "pm2-staging-reload": "pm2 sendSignal SIGUSR2 Visual_Component_Platform_Server pm2.staging.json",
    "pm2-staging-restart": "pm2 restart pm2.staging.json",
    "pm2-staging-stop": "pm2 stop pm2.staging.json",
    "pm2-development-start": "pm2 start pm2.development.json",
    "pm2-development-reload": "pm2 sendSignal SIGUSR2 Visual_Component_Platform_Server pm2.development.json",
    "pm2-development-restart": "pm2 restart pm2.development.json",
    "pm2-development-stop": "pm2 stop pm2.development.json",
    "compile": "babel --no-babelrc src/ --presets think-node --out-dir app/",
    "lint": "eslint src/",
    "lint-fix": "eslint --fix src/",
    "createWebApiDoc": "apidoc -i ./src/web/controller/ -o ./www/webApiDoc",
    "init_database_dev": "db-migrate up --config config/database.dev.json -e mysql -m ./migrations_init",
    "init_database_production": "db-migrate up --config config/database.production.json -e mysql -m ./migrations_init",
    "release": "standard-version",
    "release:minor": "npm run release -- --release-as minor",
    "release:major": "npm run release -- --release-as major",
    "replace": "replace '\\* #' '*' CHANGELOG.md",
    "changelog": "conventional-changelog --config ./node_modules/@cloudwise-fe/web-changelog/lib/log -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "adm-zip": "^0.5.5",
    "archiver": "^2.1.1",
    "async-file": "^2.0.2",
    "config": "^3.3.6",
    "db-migrate": "^0.10.2",
    "db-migrate-mysql": "^1.1.10",
    "diff2html": "^3.4.11",
    "extract-zip": "^1.6.6",
    "form-data": "^2.3.1",
    "html-minifier": "^4.0.0",
    "kitx": "^1.3.0",
    "lodash": "^4.17.4",
    "mime": "^2.2.0",
    "mysql2": "^2.3.0",
    "node-client-sdk": "^1.0.4",
    "pm2": "^2.9.1",
    "sequelize": "^6.6.5",
    "simple-git": "^2.45.1",
    "think-cache": "^1.1.0",
    "think-cache-file": "^1.1.0",
    "think-cache-redis": "^1.2.0",
    "think-logger3": "^1.1.1",
    "think-model": "^1.1.8",
    "think-model-mysql": "^1.0.6",
    "think-session": "^1.0.3",
    "think-session-file": "^1.0.6",
    "think-view": "^1.0.11",
    "think-view-nunjucks": "^1.0.7",
    "thinkjs": "^3.2.7",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@cloudwise-fe/web-changelog": "^1.0.0",
    "apidoc": "^0.17.6",
    "babel-cli": "^6.26.0",
    "babel-preset-think-node": "^1.0.3",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^4.15.0",
    "eslint-config-think": "^1.0.2",
    "node-notifier": "^5.2.1",
    "replace": "^1.2.1",
    "standard-version": "^9.3.0",
    "think-babel": "^1.0.6",
    "think-inspect": "^0.0.2",
    "think-watcher": "^3.0.3"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "yarn changelog && yarn replace && git add .",
      "posttag": "git push origin --tags -f"
    }
  },
  "apidoc": {
    "name": "example",
    "version": "1.0.0",
    "description": "apiDoc basic example",
    "apidoc": {
      "title": "Custom apiDoc browser title",
      "url": "https://api.github.com/v1"
    }
  },
  "repository": "",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "readmeFilename": "README.md"
}
